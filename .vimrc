set nocompatible
filetype off

set rtp+=~/.vim/bundle/Vundle.vim
call vundle#begin()

Plugin 'VundleVim/Vundle.vim'

call vundle#end()
filetype plugin indent on

colorscheme xcode
syntax enable

set sts=4
set nu
set cindent
set autoindent
set smartindent
set ruler
set title  
set ts=4
set incsearch
set showmatch
